@model IEnumerable<OcsicoTraining.Stasulevich.Lesson4.OrganizationManagmentSystem.Models.User>

@{
    ViewData["Title"] = "UserList";
}

<h2>Users list</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access rights</a>
            </td>
        </tr>
    }
</table>
